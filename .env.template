# Environment template for openhands-aidev-dataset
# Copy this file to `.env` and fill in the values.

# GitHub API token (recommended to avoid strict rate limits)
GITHUB_TOKEN=

# Data / output directories (relative to repo root)
DATA_DIR=analysis_outputs

# Common input filenames (relative to repo root)
AIDEV_FULL_CSV=AIDev-Full/AIDev-PRs-Full.csv
OPENHANDS_CSV=OpenHands/openhands_prs_2024-03-13_to_2025-06-11.csv
OH_MONTHLY_COMMITS=analysis_outputs/openhands_monthly_commits_2024-03-13_to_2025-06-11.csv

# Optional: logging & request settings
LOG_LEVEL=INFO
REQUESTS_TIMEOUT=10
 
# Path to AI-Dev full parquet (relative to repo root)
AIDEV_FULL_PARQUET=all_pull_request.parquet

# Output directory for analysis results (relative to repo root)
OUTPUT_DIR=analysis_outputs
